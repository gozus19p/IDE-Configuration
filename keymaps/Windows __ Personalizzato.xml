<keymap version="1" name="Windows :: Personalizzato" parent="$default">
  <action id="CommentByBlockComment">
    <keyboard-shortcut first-keystroke="shift ctrl divide" />
    <keyboard-shortcut first-keystroke="shift ctrl back_slash" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="ctrl back_slash" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="ctrl d" />
  </action>
  <action id="Generate">
    <keyboard-shortcut first-keystroke="alt insert" />
    <keyboard-shortcut first-keystroke="shift ctrl g" />
  </action>
</keymap>