<keymap version="1" name="Windows Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="ChangesView.Diff" />
  <action id="CommentByBlockComment">
    <keyboard-shortcut first-keystroke="shift ctrl slash" />
    <keyboard-shortcut first-keystroke="shift ctrl divide" />
    <keyboard-shortcut first-keystroke="shift ctrl back_slash" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="ctrl back_slash" />
  </action>
  <action id="CompareTwoFiles" />
  <action id="Console.TableResult.CloneRow" />
  <action id="DatabaseView.CopyAction" />
  <action id="Diff.ShowDiff" />
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="ctrl d" />
  </action>
  <action id="EditorDuplicate" />
  <action id="FileChooser.GotoDesktop" />
  <action id="RunDashboard.CopyConfiguration" />
  <action id="SendEOF" />
  <action id="XDebugger.CopyWatch" />
  <action id="openAssertEqualsDiff" />
</keymap>